		/ .................................... $Contact ....................................
		%section.section-content.section-contact.section-color-graylighter
			.container
				.row
					.span4
						%h5 Twitter, Inc.
						%address
							795 Folsom Ave, Suite 600
							%br/
							San Francisco, CA 94107
						%p
							%i.icon-envelope
							%a{:href => "mailto:info@company.com"} info@company.com
							%br/
							%i.icon-phone
							%a{:href => "tel:123-456-7890"} (123) 456-7890


					.span4
						%h5 Mail us
						%form.form-vertical#mail-form
							.control-group
								%label{:for => "mail-name-surname"}
								%input.span3#mail-name-surname{:type => "text", "placeholder" => "name surname"}/
