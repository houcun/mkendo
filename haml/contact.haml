							.control-group
								%label{:for => "mail-email"}
								%input.span3#mail-email{:type => "email", "placeholder" => "email"}/
								%span.help-inline Insert valid email
							%label{:for => "mail-message"}
							%textarea.span4#mail-message{:name => "message", :rows => "5", "placeholder" => "message"}
							%label.checkbox
								%input{:type => "checkbox"}/
								Sign up for newsletter
							%button.btn.btn-primary{:type => "submit"} Send message
					.span4
						%h5 Work with us!
						%p Blanditiis nobis voluptatem placeat ullam eum deleniti tenetur optio omnis facere incidunt quibusdam modi pariatur saepe maxime ab ipsum labore nihil tempore.
						%p
							%a.btn.btn-small.btn-primary{:href => "#"} Send CV
